
sudo echo "should be elevated"


APPLICATION=cicd-assistant

sudo cp $APPLICATION /etc/init.d/$APPLICATION

sudo chmod +x /etc/init.d/$APPLICATION

sudo update-rc.d $APPLICATION defaults

sudo update-rc.d $APPLICATION enable

sudo systemctl start $APPLICATION.service

sudo systemctl status $APPLICATION.service && echo "Installed Successfully"
# STX:Out: install
# STX:Rep: $1 cicd-assistant
